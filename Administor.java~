  void addItemToMenu(String foodName, String filename){
         
  }
  void viewFoodItems(String filename){
    reader = new JsonReader();
    reader.readJson(filename,"menu");
    ArrayList<Food> foodItems = this.reader.getList();
    Iterator<Food> iterator = foodItems.iterator();
    while (iterator.hasNext()) {
      System.out.println(iterator.next());
    }
  }
  
   public static void main(String[] args){
    Administor admin = new Administor();
    admin.viewFoodItems("./myJSON.JSON");
      }
  
  
  
